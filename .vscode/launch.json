    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program (Development)",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\index.js",
            "envFile": "${workspaceFolder}\\environments\\dev.env"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program (Staging)",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\index.js",
            "envFile": "${workspaceFolder}\\environments\\staging.env"
        }
    ]
}